#include<iostream>
#include<string>
using namespace std;
int main()
{
    string str1, str2;
    int i,j;
    cout<<"Enter the first string";
    getline(cin, str1);
    cout<<"Enter the second string";
    getline(cin, str2);
    i=0;
    str1 = str1 + str2;
    cout<<"Concatenated string is: "<<str1<<endl;
    return 0;
}
