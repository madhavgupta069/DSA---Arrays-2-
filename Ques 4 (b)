#include <iostream>
#include <string>
using namespace std;
int main() {
    string str1,reverse;

    cout << "Enter a string: ";
    getline(cin, str1);
    for(int i=str1.length();i>=0;i--)
    {
        reverse += str1[i];
    }
    cout << "Reversed string: " << reverse << endl;

    return 0;
}
