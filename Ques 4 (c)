#include <iostream>
#include <algorithm>
using namespace std;

bool isVowel(char ch) {
    ch = tolower(ch);
    return (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u');
}

int main() {
    string str;

    cout << "Enter a string: ";
    getline(cin, str);

    str.erase(remove_if(str.begin(), str.end(), isVowel), str.end());

    cout << "String after deleting vowels: " << str << endl;

    return 0;
}
